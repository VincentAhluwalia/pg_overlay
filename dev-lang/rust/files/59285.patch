From 0dabf8c835d61b2f7df454be362c23ba6590c761 Mon Sep 17 00:00:00 2001
From: Josh Stone <jistone@redhat.com>
Date: Mon, 18 Mar 2019 15:59:24 -0700
Subject: [PATCH] Rebase LLVM to 8.0.0 final

---
 .gitmodules                       | 2 +-
 src/llvm-project                  | 2 +-
 src/rustllvm/llvm-rebuild-trigger | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/.gitmodules b/.gitmodules
index b75e312dedf7..dd74d49828f7 100644
--- a/.gitmodules
+++ b/.gitmodules
@@ -43,7 +43,7 @@
 [submodule "src/llvm-project"]
 	path = src/llvm-project
 	url = https://github.com/rust-lang/llvm-project.git
-	branch = rustc/8.0-2019-01-16
+	branch = rustc/8.0-2019-03-18
 [submodule "src/doc/embedded-book"]
 	path = src/doc/embedded-book
 	url = https://github.com/rust-embedded/book.git
diff --git a/src/llvm-project b/src/llvm-project
index 4fc9fb8245ab..1f484cbe0e86 160000
--- a/src/llvm-project
+++ b/src/llvm-project
@@ -1 +1 @@
-Subproject commit 4fc9fb8245abe24680192535870c4522644a4212
+Subproject commit 1f484cbe0e863e9e215f1b3d7198063444d60873
diff --git a/src/rustllvm/llvm-rebuild-trigger b/src/rustllvm/llvm-rebuild-trigger
index 9ee1bceb632b..0f18c6a4ac01 100644
--- a/src/rustllvm/llvm-rebuild-trigger
+++ b/src/rustllvm/llvm-rebuild-trigger
@@ -1,4 +1,4 @@
 # If this file is modified, then llvm will be (optionally) cleaned and then rebuilt.
 # The actual contents of this file do not matter, but to trigger a change on the
 # build bots then the contents should be changed so git updates the mtime.
-2019-01-16
+2019-03-18
